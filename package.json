{
  "type": "commonjs",
  "name": "@jcbhmr/node-util",
  "description": "ðŸ§° Node.js node:util extracted for use anywhere",
  "version": "1.0.0",
  "author": "Jacob Hummer <jcbhmr@outlook.com> (https://jcbhmr.me/)",
  "files": [
    "dist"
  ],
  "homepage": "https://jcbhmr.github.io/node-util/",
  "keywords": [
    "util",
    "nodejs",
    "polyfill",
    "types",
    "typechecking",
    "introspection"
  ],
  "license": "MIT",
  "repository": "github:jcbhmr/node-util",
  "imports": {
    "#internal/*": "./src/internal/*.js",
    "#primordials": "./src/internal/$primordials.js",
    "#internalBinding(types)": "./src/internal/$internalBinding(types).js"
  },
  "exports": {
    ".": {
      "node": "./src/util-node.js",
      "default": "./src/util.js"
    },
    "./types": {
      "node": "./src/util/types-node.js",
      "default": "./src/util/types.js"
    }
  },
  "scripts": {
    "pretest": "standard --fix",
    "test": "node --test",
    "build:docs": "echo '{}' >tsconfig.json && typedoc; rm tsconfig.json"
  },
  "typedocOptions": {
    "entryPoints": [
      "src/util.d.ts",
      "src/util/types.d.ts"
    ],
    "out": "docs/dist"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "standard": "^17.1.0",
    "tsx": "^3.12.7",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3"
  }
}
