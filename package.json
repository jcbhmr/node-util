{
  "type": "commonjs",
  "name": "@jcbhmr/node-util",
  "description": "ðŸ§° Node.js node:util extracted for use anywhere",
  "version": "1.0.0",
  "author": "Jacob Hummer <jcbhmr@outlook.com> (https://jcbhmr.me/)",
  "files": [
    "dist"
  ],
  "homepage": "https://jcbhmr.github.io/node-util/",
  "keywords": [
    "util",
    "nodejs",
    "polyfill",
    "types",
    "typechecking",
    "introspection"
  ],
  "license": "MIT",
  "repository": "github:jcbhmr/node-util",
  "imports": {
    "#internal/*": "./src/internal/*.js",
    "#$*": "./src/internal/$*.js"
  },
  "exports": {
    ".": {
      "node": {
        "types": "./node-util.d.ts",
        "import": "./node-util.mjs",
        "require": "./node-util.cjs"
      },
      "default": "./src/util.js"
    },
    "./types": {
      "node": {
        "types": "./node-util-types.d.ts",
        "import": "./node-util-types.mjs",
        "require": "./node-util-types.cjs"
      },
      "default": "./src/util/types.js"
    }
  },
  "scripts": {
    "prepack": "cp node_modules/@types/node/util.d.ts node-util.d.ts && cp node_modules/@types/node/util/types.d.ts node-util-types.d.ts",
    "test": "node --test",
    "build:docs": "echo '{}' >tsconfig.json && typedoc; rm tsconfig.json"
  },
  "typedocOptions": {
    "entryPoints": [
      "src/util.d.ts",
      "src/util/types.d.ts"
    ],
    "out": "docs/dist"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3"
  }
}
